{
  "name": "@radius/cli",
  "version": "0.0.0",
  "private": true,
  "main": "./build/cli",
  "bin": {
    "radius": "./build/cli"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/assets",
    "/build"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf build",
    "test": "jest --coverage",
    "format": "prettier --loglevel warn --write \"**/*.{ts,js,json,yaml}\"",
    "format:check": "prettier --loglevel warn --check \"**/*.{ts,js,json,yaml}\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint:check": "eslint \"src/**/*.ts\""
  },
  "dependencies": {
    "clear": "0.1.0",
    "chalk": "4.1.1",
    "cli-table": "0.3.6",
    "degit": "2.8.4",
    "enquirer": "2.3.6",
    "fs-extra": "10.0.0",
    "figlet": "1.5.2",
    "figma-api": "^1.9.2",
    "ora": "5.4.1",
    "yargs": "17.0.1",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@guanghechen/chalk-logger": "1.9.0",
    "@types/clear": "0.1.2",
    "@types/cli-table": "0.3.0",
    "@types/degit": "2.8.3",
    "@types/faker": "5.5.7",
    "@types/figlet": "1.5.4",
    "@types/fs-extra": "9.0.12",
    "@types/jest": "27.4.0",
    "@types/inquirer": "8.2.0",
    "@types/node": "16.0.1",
    "@types/yargs": "17.0.2",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "jest": "27.5.1",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "pkg": "5.3.0",
    "ts-node": "10.0.0",
    "typescript": "~4.2.3"
  },
  "pkg": {
    "scripts": "build/**/*.js"
  }
}
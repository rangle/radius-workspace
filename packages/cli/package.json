{
  "name": "@radius/cli",
  "version": "0.1.0",
  "description": "Radius CLI",
  "keywords": [
    "cli",
    "radius",
    "design-systems"
  ],
  "author": "Jason Santos <jason.santos@rangle.io>",
  "homepage": "http://radius.ranglelabs.io",
  "license": "MIT",
  "main": "lib/cli.ts",
  "bin": {
    "cli": "bin/cli"
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "bin",
    "lib"
  ],
  "publishConfig": {
    "registry": "http://localhost:4873/"
  },
  "scripts": {
    "build": "tsc",
    "start": ". .env && FIGMA_TOKEN=$FIGMA_TOKEN ./bin/radius styles $FIGMA_URL",
    "dev": ". .env && RADIUS_MODE=development FIGMA_TOKEN=$FIGMA_TOKEN ts-node ./bin/radius styles $FIGMA_URL",
    "test": "jest"
  },
  "dependencies": {
    "figma-api": "^1.6.2",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "@types/node": "^16.11.1"
  }
}

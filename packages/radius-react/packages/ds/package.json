{
  "name": "ds",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "postinstall": "yarn build",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(md|mdx)$": "<rootDir>/__mocks__/mdx-mock.js"
    },
    "transform": {
      ".(ts|tsx)": "ts-jest/dist",
      "^.+\\.[tj]sx?$": "babel-jest",
      "^.+\\.mdx?$": "@storybook/addon-docs/jest-transform-mdx"
    }
  },
  "module": "dist/ds.esm.js",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@storybook/addon-actions": "^5.3.10",
    "@storybook/addon-docs": "^5.3.12",
    "@storybook/addon-knobs": "^5.3.10",
    "@storybook/addon-links": "^5.3.10",
    "@storybook/addon-storyshots": "^5.3.12",
    "@storybook/addons": "^5.3.10",
    "@storybook/react": "^5.3.10",
    "@types/jest": "^25.1.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^4.4.2",
    "@types/styled-system": "^5.1.6",
    "babel-loader": "^8.0.6",
    "husky": "^4.2.1",
    "react": "^16.12.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-dom": "^16.12.0",
    "react-is": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "styled-components": "^5.0.0",
    "styled-system": "^5.1.4",
    "ts-loader": "^6.2.1",
    "tsdx": "^0.12.3",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  }
}